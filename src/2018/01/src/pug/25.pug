extends /layout/_DefaultLayout
prepend StylesheetBlock
  link(rel="stylesheet", href="css/main.css")
prepend JavascriptBlock
  script(src="js/25.js")

block ContentBlock
  main.main
    ul.items#items(data-selected-item="none")
      li.items__item.items__item--rock(data-item='rock')
      li.items__item.items__item--scissors(data-item='scissors')
      li.items__item.items__item--paper(data-item='paper')
    button.submit#submit submit
